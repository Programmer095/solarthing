{
  "type": "message-sender<NOTE DON'T USE THIS! Use the automation program with the message-sender action!>",
  "senders": {
    "important": "config/mattermost_important.json",
    "basic": "config/mattermost.json"
  },
  "database": "../config/couchdb.json",
  "source": "default",
  "events": [
    {
      "event": {
        "type": "lowbatteryvoltage",
        "voltage": 22.8,
        "timeout": "PT120M"
      },
      "send_to": ["basic"]
    },
    {
      "event": {
        "type": "lowbatteryvoltage",
        "voltage": 22.2,
        "timeout": "PT5M"
      },
      "send_to": ["important"]
    },
    {
      "event": {
        "type": "acmodechange",
        "to": ["NO_AC"],
        "from": ["NO_AC"]
      },
      "send_to": ["basic"]
    },
    {
      "event": {
        "type": "fxchange",
        "to": ["SILENT"]
      },
      "send_to": ["basic"]
    }
  ]
}

